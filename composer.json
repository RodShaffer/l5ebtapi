{
    "name": "l5ebtapi/l5ebtapi",
    "type": "library",
    "description": "Laravel 5.2 eBay Trading API Package. Easily add the eBay Trading API capabilities to your Laravel 5.2 projects.",
    "keywords": [
        "l5ebtapi",
        "l5ebtapi"
    ],
    "homepage": "https://github.com/l5ebtapi/l5ebtapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Rod Shaffer",
            "email": "l5ebtapipkg@yahoo.com",
            "homepage": "https://github.com/l5ebtapi/l5ebtapi",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "l5ebtapi\\l5ebtapi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "l5ebtapi\\l5ebtapi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
